{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "ignoreDependencies": [
        "virtual:pwa-info",
        "virtual:pwa-register"
    ],
    "entry": [
        "src/**/*.test.ts",
        "src/**/*.test.tsx",
        "generated/**/*.ts",
        "src/index.tsx!"
    ],
    "project": [
        "src/**/*.d.ts",
        "src/**/*.test.ts",
        "src/**/*.test.tsx",
        "src/**/*.tsx!",
        "src/**/*.ts!"
    ]
}
